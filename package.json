{
    "name": "nestjs-microservices-rabbitmq",
    "version": "0.0.1",
    "description": "About RabbitMQ is an open-source and lightweight message broker which supports multiple messaging protocols.",
    "author": "Hebert Barros <hebert@hotbrains.com.br>",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "run:pm2": "pm2 start ts-node -- -P tsconfig.json ./src/main.ts",
        "doc": "npx @compodoc/compodoc -p tsconfig.json -s",
        "dev:test": "nodemon src/main.ts --debug --watch",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "prepare:enviroment": "node scripts/dev-enviroment.cjs",
        "start:worker": "node scripts/worker.cjs"
    },
    "dependencies": {
        "@nestjs/common": "8.4.6",
        "@nestjs/core": "8.4.6",
        "@nestjs/mapped-types": "*",
        "@nestjs/microservices": "^8.4.6",
        "@nestjs/platform-express": "8.4.6",
        "amqp-connection-manager": "^4.1.7",
        "amqplib": "^0.10.3",
        "cache-manager": "4.0.0",
        "class-transformer": "0.5.1",
        "class-validator": "0.13.2",
        "dotenv": "16.0.1",
        "express": "4.18.1",
        "http-status": "1.5.2",
        "nest-shared": "^0.4.53",
        "reflect-metadata": "0.1.13",
        "rimraf": "3.0.2",
        "rxjs": "7.5.5",
        "tsyringe": "4.7.0",
        "yup": "0.32.11"
    },
    "devDependencies": {
        "@babel/cli": "7.17.10",
        "@babel/core": "7.18.2",
        "@babel/node": "7.17.10",
        "@babel/preset-env": "7.20.2",
        "@compodoc/compodoc": "1.1.19",
        "@nestjs/cli": "8.2.6",
        "@nestjs/schematics": "8.0.11",
        "@nestjs/testing": "8.4.6",
        "@types/amqplib": "^0.8.2",
        "@types/dotenv": "8.2.0",
        "@types/express": "4.17.13",
        "@types/jest": "28.1.0",
        "@types/node": "17.0.39",
        "@types/supertest": "2.0.12",
        "@typescript-eslint/eslint-plugin": "5.27.0",
        "@typescript-eslint/parser": "5.27.0",
        "@typescript-eslint/typescript-estree": "5.27.0",
        "eslint": "8.16.0",
        "eslint-config-airbnb-base": "15.0.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-prettier": "4.0.0",
        "jest": "28.1.0",
        "nodemon": "2.0.16",
        "pm2": "5.2.2",
        "prettier": "2.6.2",
        "sucrase": "3.21.0",
        "supertest": "6.2.3",
        "ts-jest": "28.0.4",
        "ts-loader": "9.3.0",
        "ts-node": "10.8.0",
        "tsconfig-paths": "4.0.0",
        "typescript": "4.7.2"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
